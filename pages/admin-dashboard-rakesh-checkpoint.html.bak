                            </tbody>
                        </table>
                    </div>
                    
                    <div id="recent-payments" class="tab-content">
                        <table class="admin-table" id="recentPaymentsTable">
                            <thead>
                                <tr>
                                    <th>ID</th>
                                    <th>Donation ID</th>
                                    <th>Amount</th>
                                    <th>Method</th>
                                    <th>Email</th>
                                    <th>Date</th>
                                    <th>Actions</th>
                                </tr>
                            </thead>
                            <tbody>
                                <!-- Payment data will be loaded here -->
                                <tr>
                                    <td colspan="7" style="text-align: center;">Loading payments...</td>
                                </tr>
                            </tbody>
                        </table>
                    </div>
                    
                    <div id="recent-requirements" class="tab-content">
                        <table class="admin-table" id="recentRequirementsTable">
                            <thead>
                                <tr>
                                    <th>Req. ID</th>
                                    <th>Organization</th>
                                    <th>Contact Person</th>
                                    <th>Type</th>
                                    <th>Amount</th>
                                    <th>Date</th>
                                    <th>Actions</th>
                                </tr>
                            </thead>
                            <tbody>
                                <!-- Requirements data will be loaded here -->
                                <tr>
                                    <td colspan="7" style="text-align: center;">Loading requirements...</td>
                                </tr>
                            </tbody>
                        </table>
                    </div>
                    
                    <div id="recent-feedback" class="tab-content">
                        <table class="admin-table" id="recentFeedbackTable">
                            <thead>
                                <tr>
                                    <th>ID</th>
                                    <th>User</th>
                                    <th>Type</th>
                                    <th>Date</th>
                                    <th>Actions</th>
                                </tr>
                            </thead>
                            <tbody>
                                <!-- Feedback data will be loaded here -->
                                <tr>
                                    <td colspan="5" style="text-align: center;">Loading feedback...</td>
                                </tr>
                            </tbody>
                        </table>
                    </div>
                </div>
            `;
            
            // Set content
            contentArea.innerHTML = dashboardHTML;
            
            // Add event listeners to tab buttons
            document.querySelectorAll('.tab-btn').forEach(btn => {
                btn.addEventListener('click', function() {
                    // Update active tab button
                    document.querySelectorAll('.tab-btn').forEach(b => b.classList.remove('active'));
                    this.classList.add('active');
                    
                    // Show the corresponding tab content
                    const tabId = this.getAttribute('data-tab');
                    document.querySelectorAll('.tab-content').forEach(tab => tab.classList.remove('active'));
                    document.getElementById(tabId).classList.add('active');
                });
            });
            
            // Load data for dashboard
            fetchDashboardStats();
            loadRecentDonations();
            loadRecentPayments();
            loadRecentRequirements();
            loadRecentFeedback();
