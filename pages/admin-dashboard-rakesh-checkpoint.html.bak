        // Fetch statistics for dashboard
        function fetchDashboardStats() {
            // Fetch donation count
            fetch('/api/donations/count')
                .then(response => {
                    if (!response.ok) {
                        throw new Error('Failed to fetch donation count');
                    }
                    return response.json();
                })
                .then(data => {
                    document.getElementById('totalDonations').textContent = data.count || '0';
                })
                .catch(error => {
                    console.error('Error fetching donation count:', error);
                    document.getElementById('totalDonations').textContent = '0';
                });
            
            // Fetch payment total
            fetch('/api/payments/total')
                .then(response => {
                    if (!response.ok) {
                        throw new Error('Failed to fetch payment total');
                    }
                    return response.json();
                })
                .then(data => {
                    document.getElementById('totalPayments').textContent = '₹' + (data.total || '0');
                })
                .catch(error => {
                    console.error('Error fetching payment total:', error);
                    document.getElementById('totalPayments').textContent = '₹0';
                });
            
            // Fetch requirements count
            fetch('/api/requirements/count')
                .then(response => {
                    if (!response.ok) {
                        throw new Error('Failed to fetch requirements count');
                    }
                    return response.json();
                })
                .then(data => {
                    document.getElementById('totalRequirements').textContent = data.count || '0';
                })
                .catch(error => {
                    console.error('Error fetching requirements count:', error);
                    document.getElementById('totalRequirements').textContent = '0';
                });
            
            // Fetch feedback count
            fetch('/api/feedback/count')
                .then(response => {
                    if (!response.ok) {
                        throw new Error('Failed to fetch feedback count');
                    }
                    return response.json();
                })
                .then(data => {
                    document.getElementById('totalFeedback').textContent = data.count || '0';
                })
                .catch(error => {
                    console.error('Error fetching feedback count:', error);
                    document.getElementById('totalFeedback').textContent = '0';
                });
            
            // Fetch users count
            fetch('/api/admin/users/count')
                .then(response => {
                    if (!response.ok) {
                        throw new Error('Failed to fetch users count');
                    }
                    return response.json();
                })
                .then(data => {
                    document.getElementById('totalUsers').textContent = data.count || '0';
                })
                .catch(error => {
                    console.error('Error fetching users count:', error);
                    document.getElementById('totalUsers').textContent = '0';
                });
        }
        
        // Load recent donations
        function loadRecentDonations() {
            const tableBody = document.querySelector('#recentDonationsTable tbody');
            
            // Fetch recent donations from API
            fetch('/api/donations/recent')
                .then(response => {
                    if (!response.ok) {
                        throw new Error('Failed to fetch recent donations');
                    }
                    return response.json();
                })
                .then(data => {
                    if (data.length > 0) {
                        // Clear loading message
                        tableBody.innerHTML = '';
                        
                        // Add each donation to the table
                        data.forEach(donation => {
                            const row = document.createElement('tr');
                            
                            // Format the date
                            const date = new Date(donation.createdAt);
                            const formattedDate = `${date.toLocaleDateString()} ${date.toLocaleTimeString()}`;
                            
                            row.innerHTML = `
                                <td>${donation._id.substring(0, 8)}</td>
                                <td style="color: #000 !important; font-weight: bold;">${donation.donorName || donation.name || 'N/A'}</td>
                                <td style="color: #000 !important;">${donation.donorEmail || donation.email || 'N/A'}</td>
                                <td style="color: #000 !important;">${donation.donationType || donation.category || 'N/A'}</td>
                                <td>${formattedDate}</td>
                                <td>
                                    <button class="action-btn view-btn" data-id="${donation._id}">View</button>
                                    <button class="action-btn edit-btn" data-id="${donation._id}">Edit</button>
                                </td>
                            `;
                            
                            tableBody.appendChild(row);
                        });
                        
                        // Add event listeners for view and edit buttons
                        document.querySelectorAll('#recentDonationsTable .view-btn').forEach(btn => {
                            btn.addEventListener('click', function() {
                                const donationId = this.getAttribute('data-id');
                                viewDonationDetails(donationId);
                            });
                        });
                        
                        document.querySelectorAll('#recentDonationsTable .edit-btn').forEach(btn => {
                            btn.addEventListener('click', function() {
                                const donationId = this.getAttribute('data-id');
                                editDonation(donationId);
                            });
                        });
                    } else {
                        tableBody.innerHTML = '<tr><td colspan="6" style="text-align: center;">No donations found</td></tr>';
                    }
                })
                .catch(error => {
                    console.error('Error fetching recent donations:', error);
                    tableBody.innerHTML = '<tr><td colspan="6" style="text-align: center;">Error loading donations from database</td></tr>';
                });
        }
        
        // Set up real-time updates for donations
        function setupDonationsListener() {
            // Check for new donations every 30 seconds
            setInterval(() => {
                fetch('/api/donations/recent?timestamp=' + Date.now())
                    .then(response => response.json())
                    .then(data => {
                        if (data.length > 0) {
                            // Update dashboard stats
                            fetch('/api/donations/count')
                                .then(response => response.json())
                                .then(countData => {
                                    document.getElementById('totalDonations').textContent = countData.count || '0';
                                });
                                
                            // Update the donations table if new data is available
                            const tableBody = document.querySelector('#recentDonationsTable tbody');
                            
                            // Clear table and add new data
                            tableBody.innerHTML = '';
                            data.forEach(donation => {
                                const row = document.createElement('tr');
                                
                                // Format the date
                                const date = new Date(donation.createdAt);
                                const formattedDate = `${date.toLocaleDateString()} ${date.toLocaleTimeString()}`;
                                
