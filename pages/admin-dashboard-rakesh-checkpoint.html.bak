            const itemsPerPage = 10;
            let filteredDonations = [];
            let allDonations = [];
            
            // Attach event listeners
            document.getElementById('donationSearchBtn').addEventListener('click', filterDonations);
            document.getElementById('donationSearchInput').addEventListener('keyup', function(e) {
                if (e.key === 'Enter') {
                    filterDonations();
                }
            });
            
            document.getElementById('donationTypeFilter').addEventListener('change', filterDonations);
            document.getElementById('donationDateFilter').addEventListener('change', filterDonations);
            
            document.getElementById('donationPrevPage').addEventListener('click', function() {
                if (currentPage > 1) {
                    currentPage--;
                    displayDonations();
                }
            });
            
            document.getElementById('donationNextPage').addEventListener('click', function() {
                if (currentPage * itemsPerPage < filteredDonations.length) {
                    currentPage++;
                    displayDonations();
                }
            });
            
            document.getElementById('selectAllDonations').addEventListener('change', function() {
                const checkboxes = document.querySelectorAll('#manageDonationsTable tbody input[type="checkbox"]');
                checkboxes.forEach(checkbox => {
                    checkbox.checked = this.checked;
                });
            });
            
            document.getElementById('applyDonationBulkAction').addEventListener('click', function() {
                const action = document.getElementById('donationBulkAction').value;
                if (!action) {
                    showNotification('Please select an action to apply', 'warning');
                    return;
                }
                
                const selectedDonations = [];
                document.querySelectorAll('#manageDonationsTable tbody input[type="checkbox"]:checked').forEach(checkbox => {
                    selectedDonations.push(checkbox.getAttribute('data-id'));
                });
                
                if (selectedDonations.length === 0) {
                    showNotification('Please select at least one donation', 'warning');
                    return;
                }
                
                if (confirm(`Are you sure you want to ${action.replace('-', ' ')} ${selectedDonations.length} donation(s)?`)) {
                    applyBulkAction(action, selectedDonations);
                }
            });
            
            // Load donations data from database API
            const tableBody = document.querySelector('#manageDonationsTable tbody');
            
            // Fetch donations from API
            fetch('/api/admin/donations/all')
                .then(response => {
                    if (!response.ok) {
                        throw new Error('Failed to fetch donations data');
                    }
                    return response.json();
                })
                .then(donations => {
                    allDonations = donations;
                    filteredDonations = [...donations];
                    
                    // Update pagination info
                    document.getElementById('donationTotal').textContent = donations.length;
                    
                    // Display donations
                    displayDonations();
                })
                .catch(error => {
                    console.error('Error loading donations:', error);
                    tableBody.innerHTML = '<tr><td colspan="7" style="text-align: center;">Error loading donations from database. Please check your connection and try again.</td></tr>';
                });
            
            // Function to display donations
            function displayDonations() {
                tableBody.innerHTML = '';
                
                const start = (currentPage - 1) * itemsPerPage;
                const end = Math.min(start + itemsPerPage, filteredDonations.length);
                
                // Update pagination info
                document.getElementById('donationStart').textContent = filteredDonations.length > 0 ? start + 1 : 0;
                document.getElementById('donationEnd').textContent = end;
                document.getElementById('donationCurrentPage').textContent = `Page ${currentPage}`;
                
                if (filteredDonations.length === 0) {
                    tableBody.innerHTML = '<tr><td colspan="7" style="text-align: center;">No donations found</td></tr>';
                    return;
                }
                
                // Display donations for current page
                for (let i = start; i < end; i++) {
                    const donation = filteredDonations[i];
                    
                    // Format date
                    const date = new Date(donation.createdAt || donation.timestamp);
                    const formattedDate = date.toLocaleDateString();
                    
                    const row = document.createElement('tr');
                    row.innerHTML = `
                        <td><input type="checkbox" data-id="${donation._id || donation.id}"></td>
                        <td>${(donation._id || donation.id).substring(0, 8)}</td>
                        <td>${donation.donorName || donation.name || 'N/A'}</td>
                        <td>${donation.donorEmail || donation.email || 'N/A'}</td>
