            const modal = document.getElementById('contact-detail-modal');
            const modalContent = document.getElementById('contact-detail-content');
            
            // Show loading state
            modalContent.innerHTML = '<div class="loading">Loading contact details...</div>';
            
            // Display the modal
            modal.style.display = 'block';
            
            // Fetch the contact details
            fetch(`/api/contact/${contactId}`)
                .then(response => {
                    if (!response.ok) {
                        throw new Error('Failed to fetch contact details');
                    }
                    return response.json();
                })
                .then(contact => {
                    // Format the date
                    const date = new Date(contact.createdAt).toLocaleString();
                    
                    // Create the HTML for the contact details
                    let detailsHTML = `
                        <div class="contact-detail-header">
                            <div class="contact-meta">
                                <p><strong>ID:</strong> ${contact._id}</p>
                                <p><strong>Name:</strong> ${contact.name}</p>
