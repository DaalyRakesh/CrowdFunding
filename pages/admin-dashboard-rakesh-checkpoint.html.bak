        // Function to update requirement status
        function updateRequirementStatus(requirementId) {
            // Fetch current requirement first to get current status
            fetch(`/api/admin/requirements/${requirementId}`)
                .then(response => {
                    if (!response.ok) {
                        throw new Error('Failed to fetch requirement');
                    }
                    return response.json();
                })
                .then(requirement => {
                    // Current status
                    const currentStatus = requirement.status || 'pending';
                    
                    // Create modal for status update
                    const modalHTML = `
                        <div class="modal-header">
                            <h2>Update Requirement Status</h2>
                            <span class="close">&times;</span>
                        </div>
                        <div class="modal-body">
                            <form id="updateRequirementForm">
                                <div class="form-group">
                                    <label for="requirementStatus">Status:</label>
                                    <select id="requirementStatus" name="status">
                                        <option value="pending" ${currentStatus === 'pending' ? 'selected' : ''}>Pending</option>
                                        <option value="in_progress" ${currentStatus === 'in_progress' ? 'selected' : ''}>In Progress</option>
                                        <option value="fulfilled" ${currentStatus === 'fulfilled' ? 'selected' : ''}>Fulfilled</option>
                                        <option value="rejected" ${currentStatus === 'rejected' ? 'selected' : ''}>Rejected</option>
                                    </select>
                                </div>
                                <div class="form-group">
                                    <label for="requirementNotes">Notes:</label>
                                    <textarea id="requirementNotes" name="notes" rows="3"></textarea>
                                </div>
                                <div class="form-actions">
                                    <button type="button" class="btn secondary-btn" onclick="closeModal()">Cancel</button>
                                    <button type="submit" class="btn primary-btn">Update Status</button>
                                </div>
                            </form>
                        </div>
                    `;
                    
                    showModal(modalHTML);
                    
                    // Add event listener to form
                    document.getElementById('updateRequirementForm').addEventListener('submit', function(e) {
                        e.preventDefault();
                        
                        const newStatus = document.getElementById('requirementStatus').value;
                        const notes = document.getElementById('requirementNotes').value;
                        
                        // Send update to API
                        fetch(`/api/admin/requirements/${requirementId}/status`, {
                            method: 'PUT',
                            headers: {
                                'Content-Type': 'application/json'
                            },
                            body: JSON.stringify({
                                status: newStatus,
                                notes: notes
                            })
                        })
                        .then(response => {
                            if (!response.ok) {
                                throw new Error('Failed to update requirement status');
                            }
                            return response.json();
                        })
                        .then(data => {
                            closeModal();
                            showNotification('Requirement status updated successfully', 'success');
                            
                            // Refresh the requirements table
                            loadRequirementsManageContent();
                        })
                        .catch(error => {
                            console.error('Error updating requirement status:', error);
                            showNotification('Failed to update requirement status', 'error');
                        });
                    });
                })
                .catch(error => {
                    console.error('Error fetching requirement for update:', error);
                    showNotification('Failed to update requirement', 'error');
                });
        }
        
        // Function to load and manage feedback from real database
        function loadFeedbackManageContent() {
            const contentArea = document.getElementById('adminContent');
            
            contentArea.innerHTML = `
                <h2 class="page-title"><i class="fas fa-comments"></i> Manage Feedback</h2>
                
                <div class="table-container">
                    <table class="admin-table" id="manageFeedbackTable">
                        <thead>
                            <tr>
                                <th>ID</th>
                                <th>User</th>
                                <th>Email</th>
                                <th>Type</th>
                                <th>Date</th>
                                
                                <th>Actions</th>
                            </tr>
                        </thead>
                        <tbody>
                            <tr>
                                <td colspan="7" style="text-align: center;">Loading feedback...</td>
                            </tr>
                        </tbody>
                    </table>
                </div>
            `;
            
            // Load feedback data from database API
            const tableBody = document.querySelector('#manageFeedbackTable tbody');
            
            // Fetch feedback from API
            fetch('/api/admin/feedback/all')
                .then(response => {
                    if (!response.ok) {
                        throw new Error('Failed to fetch feedback data');
                    }
                    return response.json();
                })
                .then(feedbackEntries => {
                    if (feedbackEntries && feedbackEntries.length > 0) {
                        tableBody.innerHTML = '';
                        
                        feedbackEntries.forEach(feedback => {
                            const row = document.createElement('tr');
                            
                            // Format date
                            const date = new Date(feedback.createdAt || feedback.timestamp);
                            const formattedDate = date.toLocaleDateString();
                            
                            // Determine status class
                            let statusClass = '';
                            const status = feedback.status || 'new';
                            
                            switch (status.toLowerCase()) {
                                case 'new':
                                    statusClass = 'new';
                                    break;
                                case 'in-progress':
                                case 'in_progress':
                                    statusClass = 'in_progress';
                                    break;
                                case 'resolved':
                                    statusClass = 'resolved';
                                    break;
                                default:
                                    statusClass = 'new';
                            }
                            
                            row.innerHTML = `
                                <td>${(feedback._id || feedback.id).substring(0, 8)}</td>
                                <td>${feedback.userName || feedback.name || 'Anonymous'}</td>
                                <td>${feedback.email || feedback.userEmail || 'N/A'}</td>
                                <td>${feedback.type || feedback.feedbackType || 'General'}</td>
                                <td>${formattedDate}</td>
                                
                                <td>
                                    <button class="action-btn view-feedback-btn" data-id="${feedback._id || feedback.id}">View</button>
                                    <button class="action-btn respond-feedback-btn" data-id="${feedback._id || feedback.id}">Respond</button>
                                </td>
                            `;
                            
                            tableBody.appendChild(row);
                        });
                        
                        // Add event listeners for action buttons
                        attachFeedbackEventListeners();
                    } else {
                        tableBody.innerHTML = '<tr><td colspan="7" style="text-align: center;">No feedback found</td></tr>';
                    }
                })
                .catch(error => {
                    console.error('Error loading feedback:', error);
                    tableBody.innerHTML = '<tr><td colspan="7" style="text-align: center;">Error loading feedback from database. Please check your connection and try again.</td></tr>';
                });
        }
        
        // Helper function to attach event listeners to feedback table elements
        function attachFeedbackEventListeners() {
            // View button listeners
            document.querySelectorAll('#manageFeedbackTable .view-feedback-btn').forEach(btn => {
                btn.addEventListener('click', function() {
                    const feedbackId = this.getAttribute('data-id');
                    viewFeedbackDetails(feedbackId);
                });
            });
            
            // Respond button listeners
            document.querySelectorAll('#manageFeedbackTable .respond-feedback-btn').forEach(btn => {
                btn.addEventListener('click', function() {
                    const feedbackId = this.getAttribute('data-id');
                    respondToFeedback(feedbackId);
                });
            });
        }
        
        // Function to show a modal
        function showModal(content) {
            // Create modal container if it doesn't exist
            let modal = document.getElementById('admin-modal');
            if (!modal) {
                modal = document.createElement('div');
                modal.id = 'admin-modal';
                modal.className = 'modal';
                document.body.appendChild(modal);
            }
            
            // Set modal content
            modal.innerHTML = `<div class="modal-content">${content}</div>`;
            
            // Display the modal
            modal.style.display = 'block';
            
            // Add close button event listener
            const closeBtn = modal.querySelector('.close');
            if (closeBtn) {
                closeBtn.addEventListener('click', closeModal);
            }
            
            // Close when clicking outside the modal content
            window.addEventListener('click', function(event) {
                if (event.target === modal) {
                    closeModal();
                }
            });
        }
        
        // Function to close the modal
        function closeModal() {
            const modal = document.getElementById('admin-modal');
            if (modal) {
                modal.style.display = 'none';
            }
        }
        
        // Function to show notification
        function showNotification(message, type = 'info') {
            // Create notification element if it doesn't exist
            let notification = document.getElementById('notification');
            if (!notification) {
                notification = document.createElement('div');
                notification.id = 'notification';
                document.body.appendChild(notification);
            }
            
            // Set notification content and type
            notification.textContent = message;
            notification.className = `notification ${type}`;
            
            // Display the notification
            notification.style.display = 'block';
            
            // Hide after 3 seconds
            setTimeout(() => {
                notification.style.display = 'none';
            }, 3000);
        }
        
        // Function to view donation details
        function viewDonationDetails(donationId) {
            // Fetch donation details from API
            fetch(`/api/admin/donations/${donationId}`)
                .then(response => {
                    if (!response.ok) {
                        throw new Error('Failed to fetch donation details');
                    }
                    return response.json();
                })
                .then(donation => {
                    // Format date
                    const date = new Date(donation.createdAt || donation.timestamp);
                    const formattedDate = date.toLocaleString();
                    
                    // Create modal HTML
                    const modalHTML = `
                        <div class="modal-header">
                            <h2>Donation Details</h2>
                            <span class="close">&times;</span>
                        </div>
                        <div class="modal-body">
                            <div class="donation-details">
                                <div class="detail-row">
                                    <div class="detail-group">
                                        <label>Donation ID:</label>
                                        <div class="detail-value">${donation._id || donation.id}</div>
                                    </div>
                                    <div class="detail-group">
                                        <label>Date:</label>
                                        <div class="detail-value">${formattedDate}</div>
                                    </div>
                                </div>
                                
                                <div class="detail-row">
                                    <div class="detail-group">
                                        <label>Donor Name:</label>
                                        <div class="detail-value">${donation.donorName || donation.name || 'N/A'}</div>
                                    </div>
                                    <div class="detail-group">
                                        <label>Donor Email:</label>
                                        <div class="detail-value">${donation.donorEmail || donation.email || 'N/A'}</div>
                                    </div>
                                </div>
                                
                                <div class="detail-row">
                                    <div class="detail-group">
                                        <label>Category:</label>
                                        <div class="detail-value">${donation.donationType || donation.category || 'N/A'}</div>
                                    </div>
                                    <div class="detail-group">
                                        <label>Status:</label>
                                        <div class="detail-value">
                                            ${donation.status || "Pending"}
                                        </div>
                                    </div>
                                </div>
                                
                                <div class="detail-row">
                                    <div class="detail-group full-width">
                                        <label>Description:</label>
                                        <div class="detail-value">${donation.donationDescription || donation.description || 'No description provided'}</div>
                                    </div>
                                </div>
                                
                                <div class="detail-row">
                                    <div class="detail-group full-width">
                                        <label>Additional Details:</label>
                                        <div class="detail-value">
                                            <pre>${JSON.stringify(donation, null, 2)}</pre>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            
                            <div class="related-data">
                                <h3>Related Payments</h3>
                                <div id="related-payments-container">
                                    <p class="loading">Loading related payments...</p>
                                </div>
                            </div>
                            
                            <div class="modal-actions">
                                <button class="btn secondary-btn" onclick="closeModal()">Close</button>
                                <button class="btn primary-btn" onclick="editDonation('${donation._id || donation.id}')">Edit Donation</button>
                                <button class="btn danger-btn" onclick="confirmDeleteDonation('${donation._id || donation.id}')">Delete Donation</button>
                            </div>
                        </div>
                    `;
                    
                    // Display modal
                    showModal(modalHTML);
                    
                    // Load related payments
                    loadRelatedPayments(donationId);
                })
                .catch(error => {
                    console.error('Error fetching donation details:', error);
                    showNotification('Failed to load donation details', 'error');
                });
        }
        
        // Function to load related payments for a donation
        function loadRelatedPayments(donationId) {
            const container = document.getElementById('related-payments-container');
            
            // Fetch related payments from API
            fetch(`/api/admin/payments/by-donation/${donationId}`)
                .then(response => {
                    if (!response.ok) {
                        throw new Error('Failed to fetch related payments');
                    }
                    return response.json();
                })
                .then(payments => {
                    if (payments && payments.length > 0) {
                        let html = '<table class="related-data-table">';
                        html += '<thead><tr><th>Payment ID</th><th>Amount</th><th>Method</th><th>Date</th></tr></thead>';
                        html += '<tbody>';
                        
                        payments.forEach(payment => {
                            const date = new Date(payment.createdAt || payment.timestamp);
                            const formattedDate = date.toLocaleDateString();
                            
                            html += `
                                <tr>
                                    <td>${payment._id.substring(0, 8)}</td>
                                    <td>₹${payment.amount || '0'}</td>
                                    <td>${payment.paymentMethod || payment.method || 'N/A'}</td>
                                    <td>${formattedDate}</td>
                                    
                                </tr>
                            `;
                        });
                        
                        html += '</tbody></table>';
                        container.innerHTML = html;
                    } else {
                        container.innerHTML = '<p class="no-data">No related payments found</p>';
                    }
                })
                .catch(error => {
                    console.error('Error fetching related payments:', error);
                    container.innerHTML = '<p class="error">Failed to load related payments</p>';
                });
        }
        
        // Function to edit donation
        function editDonation(donationId) {
            // Fetch donation details from API
            fetch(`/api/admin/donations/${donationId}`)
                .then(response => {
                    if (!response.ok) {
                        throw new Error('Failed to fetch donation details');
                    }
                    return response.json();
                })
                .then(donation => {
                    // Create modal HTML with edit form
                    const modalHTML = `
                        <div class="modal-header">
                            <h3><i class="fas fa-edit"></i> Edit Donation</h3>
                            <button class="close-modal"><i class="fas fa-times"></i></button>
                        </div>
                        <div class="modal-body">
                            <form id="editDonationForm">
                                <div class="form-group">
                                    <label for="donorName">Donor Name</label>
                                    <input type="text" id="donorName" name="donorName" value="${donation.donorName || donation.name || ''}" required>
                                </div>
                                <div class="form-group">
                                    <label for="donorEmail">Donor Email</label>
                                    <input type="email" id="donorEmail" name="donorEmail" value="${donation.donorEmail || donation.email || ''}" required>
                                </div>
                                <div class="form-group">
                                    <label for="donationType">Donation Type</label>
                                    <select id="donationType" name="donationType" required>
                                        <option value="Food" ${(donation.donationType || donation.category) === 'Food' ? 'selected' : ''}>Food</option>
                                        <option value="Clothing" ${(donation.donationType || donation.category) === 'Clothing' ? 'selected' : ''}>Clothing</option>
                                        <option value="Money" ${(donation.donationType || donation.category) === 'Money' ? 'selected' : ''}>Money</option>
                                        <option value="Other" ${(donation.donationType || donation.category) === 'Other' ? 'selected' : ''}>Other</option>
                                    </select>
                                </div>
                                <div class="form-group">
                                    <label for="description">Description</label>
                                    <textarea id="description" name="description" rows="3">${donation.description || ''}</textarea>
                                </div>
                                <div class="form-actions">
                                    <button type="submit" class="btn primary-btn">Update Donation</button>
                                    <button type="button" class="btn secondary-btn close-modal">Cancel</button>
                                </div>
                            </form>
                        </div>
                    `;

                    // Show modal
                    showModal(modalHTML);

                    // Set up form submission
                    document.getElementById('editDonationForm').addEventListener('submit', function(e) {
                        e.preventDefault();
                        
                        const updatedDonation = {
                            donorName: document.getElementById('donorName').value,
                            donorEmail: document.getElementById('donorEmail').value,
                            donationType: document.getElementById('donationType').value,
                            description: document.getElementById('description').value
                        };
                        
                        // Update donation via API
                        fetch(`/api/admin/donations/${donationId}`, {
                            method: 'PUT',
                            headers: {
                                'Content-Type': 'application/json'
                            },
                            body: JSON.stringify(updatedDonation)
                        })
                        .then(response => {
                            if (!response.ok) {
                                throw new Error('Failed to update donation');
                            }
                            return response.json();
                        })
                        .then(data => {
                            closeModal();
                            showNotification('Donation updated successfully', 'success');
                            
                            // Refresh donation data
                            if (document.getElementById('manageDonationsTable')) {
                                // Refresh donations management table
                                fetch('/api/admin/donations/all')
                                    .then(response => response.json())
                                    .then(donations => {
                                        window.allDonations = donations;
                                        window.filterDonations ? window.filterDonations() : loadDonationsManageContent();
                                    });
                            } else {
                                // Refresh dashboard
                                loadRecentDonations();
                            }
                        })
                        .catch(error => {
                            console.error('Error updating donation:', error);
                            showNotification('Failed to update donation. Please try again.', 'error');
                        });
                    });
                })
                .catch(error => {
                    console.error('Error fetching donation details:', error);
                    showNotification('Failed to load donation details. Please try again.', 'error');
                });
        }
        
        // Function to confirm deletion of a donation
        function confirmDeleteDonation(donationId) {
            if (confirm('Are you sure you want to delete this donation? This action cannot be undone.')) {
                deleteDonation(donationId);
            }
        }
        
        // Function to delete a donation
        function deleteDonation(donationId) {
            fetch(`/api/admin/donations/${donationId}`, {
                method: 'DELETE'
            })
            .then(response => {
                if (!response.ok) {
                    throw new Error('Failed to delete donation');
                }
                return response.json();
            })
            .then(data => {
                closeModal();
                showNotification('Donation deleted successfully', 'success');
                
                // Refresh data
                if (document.querySelector('.nav-item.active').getAttribute('data-tab') === 'donations-manage') {
                    loadDonationsManageContent();
                } else {
                    loadRecentDonations();
                }
            })
            .catch(error => {
                console.error('Error deleting donation:', error);
                showNotification('Failed to delete donation', 'error');
            });
        }
    </script>
