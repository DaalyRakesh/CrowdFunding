                            <button class="action-btn edit-donation-btn" data-id="${donation._id || donation.id}"><i class="fas fa-edit"></i></button>
                            <button class="action-btn delete-donation-btn" data-id="${donation._id || donation.id}"><i class="fas fa-trash"></i></button>
                        </td>
                    `;
                    
                    tableBody.appendChild(row);
                }
                
                // Add event listeners for action buttons
                attachDonationEventListeners();
            }
            
            // Function to filter donations
            function filterDonations() {
                const searchText = document.getElementById('donationSearchInput').value.toLowerCase();
                const typeFilter = document.getElementById('donationTypeFilter').value;
                const dateFilter = document.getElementById('donationDateFilter').value;
                
                filteredDonations = allDonations.filter(donation => {
                    // Search text filter
                    const nameMatch = (donation.donorName || donation.name || '').toLowerCase().includes(searchText);
                    const emailMatch = (donation.donorEmail || donation.email || '').toLowerCase().includes(searchText);
                    const typeMatch = (donation.donationType || donation.category || '').toLowerCase().includes(searchText);
                    const textMatch = nameMatch || emailMatch || typeMatch || searchText === '';
                    
                    // Type filter
                    const type = donation.donationType || donation.category || '';
                    const typeMatch2 = typeFilter === 'all' || type === typeFilter;
                    
                    // Date filter
                    let dateMatch = true;
                    const donationDate = new Date(donation.createdAt || donation.timestamp);
                    const today = new Date();
                    
                    if (dateFilter === 'today') {
                        dateMatch = donationDate.toDateString() === today.toDateString();
                    } else if (dateFilter === 'week') {
                        const weekStart = new Date(today);
                        weekStart.setDate(today.getDate() - today.getDay());
                        dateMatch = donationDate >= weekStart;
                    } else if (dateFilter === 'month') {
                        dateMatch = donationDate.getMonth() === today.getMonth() && 
                                   donationDate.getFullYear() === today.getFullYear();
                    } else if (dateFilter === 'year') {
                        dateMatch = donationDate.getFullYear() === today.getFullYear();
                    }
                    
                    return textMatch && typeMatch2 && dateMatch;
                });
                
                // Reset to first page
                currentPage = 1;
                
                // Update pagination info
                document.getElementById('donationTotal').textContent = filteredDonations.length;
                
                // Display filtered donations
                displayDonations();
            }
            
            // Function to apply bulk actions
            function applyBulkAction(action, donationIds) {
                let endpoint = '';
                let method = 'DELETE';
                let body = {};
                
                if (action === 'delete') {
                    endpoint = '/api/admin/donations/bulk-delete';
                    body = { ids: donationIds };
                } else {
                    return;
                }
                
                fetch(endpoint, {
                    method: method,
                    headers: {
                        'Content-Type': 'application/json'
                    },
                    body: JSON.stringify(body)
                })
                .then(response => {
                    if (!response.ok) {
                        throw new Error('Failed to apply bulk action');
                    }
                    return response.json();
                })
                .then(data => {
                    showNotification(`${data.message || 'Bulk action applied successfully'}`, 'success');
                    
                    // Reload donations
                    fetch('/api/admin/donations/all')
                        .then(response => response.json())
                        .then(donations => {
                            allDonations = donations;
                            filterDonations(); // Re-apply filters
                        });
                })
                .catch(error => {
                    console.error('Error applying bulk action:', error);
                    showNotification('Failed to apply bulk action. Please try again.', 'error');
                });
            }
        }
        
