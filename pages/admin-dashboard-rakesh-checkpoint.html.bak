        /**
         * Displays the feedback details in a modal
         * @param {number} feedbackId - The ID of the feedback to view
         */
        function viewFeedbackDetails(feedbackId) {
            // Get the modal
            const modal = document.getElementById('feedback-detail-modal');
            const modalContent = document.getElementById('feedback-detail-content');
            
            // Show loading state
            modalContent.innerHTML = '<div class="loading">Loading feedback details...</div>';
            
            // Display the modal
            modal.style.display = 'block';
            
            // Fetch the feedback details
            fetch(`/api/admin/feedback/${feedbackId}`)
                .then(response => {
                    if (!response.ok) {
                        throw new Error('Failed to fetch feedback details');
                    }
                    return response.json();
                })
                .then(feedback => {
                    // Format the date
                    const date = new Date(feedback.timestamp || feedback.createdAt).toLocaleString();
                    
                    // Create the HTML for the feedback details
                    let detailsHTML = `
                        <div class="feedback-detail-header">
                            <div class="feedback-meta">
                                <p><strong>ID:</strong> ${feedback.id || feedback._id}</p>
                                <p><strong>User:</strong> ${feedback.userName || feedback.name || feedback.fullName || 'Anonymous'}</p>
                                <p><strong>Email:</strong> ${feedback.email || feedback.feedbackEmail || 'N/A'}</p>
                                <p><strong>Phone:</strong> ${feedback.phoneNo || 'N/A'}</p>
                                <p><strong>Type:</strong> ${feedback.type || feedback.feedbackType || 'General'}</p>
                                <p><strong>Date:</strong> ${date}</p>
                                <p><strong>Status:</strong> <span class="status status-new">New</span></p>
                            </div>
                        </div>
                        <div class="feedback-message">
                            <h3>Feedback Message:</h3>
                            <p>${feedback.message || feedback.feedbackText || 'No message provided'}</p>
                        </div>
                    `;
                    
                    // If there are responses, show them
                    if (feedback.responses && feedback.responses.length > 0) {
                        detailsHTML += '<div class="feedback-responses"><h3>Responses:</h3>';
                        
                        feedback.responses.forEach(response => {
                            const responseDate = new Date(response.timestamp).toLocaleString();
                            detailsHTML += `
                                <div class="response-item">
                                    <p class="response-meta">
                                        <strong>${response.adminName}</strong> on ${responseDate}
                                    </p>
                                    <p class="response-message">${response.message}</p>
                                </div>
                            `;
                        });
                        
                        detailsHTML += '</div>';
                    }
                    
                    // Add buttons for actions
                    detailsHTML += `
                        <div class="feedback-actions">
                            <button class="btn secondary-btn close-modal">Close</button>
                            <button class="btn primary-btn" onclick="respondToFeedback('${feedback.id || feedback._id}')">Respond</button>
                        </div>
                    `;
                    
                    // Update the modal content
                    modalContent.innerHTML = detailsHTML;
                    
                    // Add event listener to the close button
                    document.querySelectorAll('#feedback-detail-modal .close-modal').forEach(button => {
                        button.addEventListener('click', function() {
                            modal.style.display = 'none';
                        });
                    });
                })
                .catch(error => {
                    console.error('Error fetching feedback details:', error);
                    modalContent.innerHTML = `
                        <div class="error-message">
                            <p>Failed to load feedback details. Please try again.</p>
                            <button class="btn secondary-btn close-modal">Close</button>
                        </div>
                    `;
                    
                    // Add event listener to the close button
                    document.querySelectorAll('#feedback-detail-modal .close-modal').forEach(button => {
                        button.addEventListener('click', function() {
                            modal.style.display = 'none';
                        });
                    });
                });
            
            // Get the close button in the modal header
            const closeButton = modal.querySelector('.close');
            
            // When the user clicks on the close button, close the modal
            closeButton.onclick = function() {
                modal.style.display = 'none';
            };
            
            // When the user clicks anywhere outside of the modal content, close it
            window.onclick = function(event) {
                if (event.target === modal) {
                    modal.style.display = 'none';
                }
            };
        }
        
        /**
         * Opens the feedback response modal to allow admins to respond to feedback
         * @param {number} feedbackId - The ID of the feedback to respond to
         */
        function respondToFeedback(feedbackId) {
            // Get the modal
            const modal = document.getElementById('feedback-response-modal');
            const messageContainer = document.getElementById('feedback-message-container');
            
            // Show loading state
            messageContainer.innerHTML = '<div class="loading">Loading feedback details...</div>';
            
            // Display the modal
            modal.style.display = 'block';
            
            // Fetch the feedback details
            fetch(`/api/admin/feedback/${feedbackId}`)
                .then(response => {
                    if (!response.ok) {
                        throw new Error('Failed to fetch feedback details');
                    }
                    return response.json();
                })
                .then(feedback => {
                    // Format the date
                    const date = new Date(feedback.timestamp || feedback.createdAt).toLocaleString();
                    
                    // Create the HTML for the feedback message
                    messageContainer.innerHTML = `
                        <div class="feedback-meta">
                            <p><strong>From:</strong> ${feedback.userName || feedback.name || feedback.fullName || 'Anonymous'}</p>
                            <p><strong>Date:</strong> ${date}</p>
                            <p><strong>Type:</strong> ${feedback.type || feedback.feedbackType || 'General'}</p>
                        </div>
                        <div class="feedback-text">
                            <p>${feedback.message || feedback.feedbackText || 'No message provided'}</p>
                        </div>
                    `;
                    
                    // Update the form action to include the feedback ID
                    const form = document.getElementById('feedback-response-form');
                    form.setAttribute('data-id', feedback.id || feedback._id);
                    
                    // Add event listener to the form submit
                    form.addEventListener('submit', function(e) {
                        e.preventDefault();
                        
                        // Get the response message
                        const responseMessage = document.getElementById('feedback-response-message').value;
                        
                        // Show loading state
                        const submitBtn = document.getElementById('submit-response');
                        submitBtn.disabled = true;
                        submitBtn.textContent = 'Sending...';
                        
                        // Send the response
                        fetch(`/api/admin/feedback/${feedbackId}/respond`, {
                            method: 'POST',
                            headers: {
                                'Content-Type': 'application/json'
                            },
                            body: JSON.stringify({
                                message: responseMessage,
                                adminName: 'Admin'
                            })
                        })
                        .then(response => {
                            if (!response.ok) {
                                throw new Error('Failed to send response');
                            }
                            return response.json();
                        })
                        .then(data => {
                            // Close the modal
                            modal.style.display = 'none';
                            
                            // Reset the form
                            form.reset();
                            
                            // Show success notification
                            showNotification('Response sent successfully', 'success');
                            
                            // Reload feedback data
                            loadRecentFeedback();
                        })
                        .catch(error => {
                            console.error('Error sending response:', error);
                            
                            // Enable the submit button
                            submitBtn.disabled = false;
                            submitBtn.textContent = 'Send Response';
                            
                            // Show error notification
                            showNotification('Failed to send response. Please try again.', 'error');
                        });
                    });
                })
                .catch(error => {
                    console.error('Error fetching feedback details:', error);
                    messageContainer.innerHTML = `
                        <div class="error-message">
                            <p>Failed to load feedback details. Please try again.</p>
                        </div>
                    `;
                });
            
            // Get the close button in the modal header
            const closeButton = modal.querySelector('.close');
            
            // When the user clicks on the close button, close the modal and reset form
            closeButton.onclick = function() {
                modal.style.display = 'none';
                document.getElementById('feedback-response-form').reset();
            };
            
            // When the user clicks anywhere outside of the modal content, close it and reset form
            window.onclick = function(event) {
                if (event.target === modal) {
                    modal.style.display = 'none';
                    document.getElementById('feedback-response-form').reset();
                }
            };
        }
        
        // Function to show notification
        function showNotification(message, type = 'info') {
            const notification = document.createElement('div');
            notification.classList.add('notification', `notification-${type}`);
            notification.textContent = message;
            
            // Add notification to the body
            document.body.appendChild(notification);
            
            // Remove after 3 seconds
            setTimeout(() => {
                notification.classList.add('fade-out');
                setTimeout(() => {
                    notification.remove();
                }, 500);
            }, 3000);
        }
        
