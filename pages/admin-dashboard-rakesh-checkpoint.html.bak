    <script>
        document.addEventListener('DOMContentLoaded', function() {
            // Initialize user data
            const adminName = localStorage.getItem('userName') || 'Admin User';
            document.getElementById('adminName').textContent = adminName;
            
            // Load dashboard content by default
            loadDashboardContent();
            
            // Set up tabs
            setupTabs();
            
            // Set up logout button
            document.getElementById('logoutBtn').addEventListener('click', function() {
                logout();
            });
        });
        
        function initializeDashboard() {
            // Fetch dashboard statistics
            fetch('/api/admin/dashboard/stats')
                .then(response => response.json())
                .then(data => {
                    // Update dashboard numbers
                    document.getElementById('totalDonations').textContent = data.donationCount || '0';
                    document.getElementById('totalPayments').textContent = '₹' + (data.paymentTotal || '0');
                    document.getElementById('totalRequirements').textContent = data.requirementCount || '0';
                    document.getElementById('totalFeedback').textContent = data.feedbackCount || '0';
                })
                .catch(error => {
                    console.error('Error fetching dashboard stats:', error);
                    // Use placeholder data on error
                    document.getElementById('totalDonations').textContent = '--';
                    document.getElementById('totalPayments').textContent = '₹--';
                    document.getElementById('totalRequirements').textContent = '--';
                    document.getElementById('totalFeedback').textContent = '--';
                });
        }
        
        // Set up tabs
        function setupTabs() {
            document.querySelectorAll('.nav-item').forEach(item => {
                item.addEventListener('click', function(e) {
                    e.preventDefault();
                    
                    // Remove active class from all tabs
                    document.querySelectorAll('.nav-item').forEach(tab => {
                        tab.classList.remove('active');
                    });
                    
                    // Add active class to clicked tab
                    this.classList.add('active');
                    
                    // Get the tab name
                    const tabName = this.getAttribute('data-tab');
                    
                    // Show the corresponding tab content
                    showTab(tabName);
                });
            });
        }
        
        function showTab(tabName) {
            // Hide all content sections
            document.querySelectorAll('.content-section').forEach(section => {
                section.style.display = 'none';
            });
            
            // Show the selected tab
            switch (tabName) {
                case 'dashboard':
                    initializeDashboard(); // Refresh dashboard data
                    break;
                case 'donations':
                    loadDonationsContent();
                    break;
                case 'payments':
                    loadPaymentsContent();
                    break;
                case 'requirements':
                    loadRequirementsContent();
                    break;
                case 'feedback':
                    loadFeedbackContent();
                    break;
                case 'donations-manage':
                    loadDonationsManageContent();
                    break;
                case 'payments-manage':
                    loadPaymentsManageContent();
                    break;
                case 'requirements-manage':
                    loadRequirementsManageContent();
                    break;
                case 'feedback-manage':
                    loadFeedbackManageContent();
                    break;
                case 'contacts':
                    loadContactContent();
                    break;
                case 'users':
                    loadUsersContent();
                    break;
                case 'settings':
                    loadSettingsContent();
                    break;
            }
        }
        
        function showDashboardTab() {
            document.querySelector('.nav-item[data-tab="dashboard"]').click();
        }
        
        // Check if user is logged in as admin
        function isLoggedInAsAdmin() {
            const isAdmin = localStorage.getItem('isAdmin') === 'true';
            
            if (!isAdmin) {
                console.error('Unauthorized access attempt to admin panel');
                return false;
            }
            
            // Additional check - verify with the server that this user is actually an admin
            const userEmail = localStorage.getItem('userEmail');
            if (!userEmail) {
                console.error('No user email found in localStorage');
                return false;
            }
            
            // This is an additional security check, but we'll still return true
            // to avoid blocking the UI while the check is in progress
            fetch(`/api/auth/check-admin?email=${encodeURIComponent(userEmail)}`)
                .then(response => response.json())
                .then(data => {
                    if (!data.isAdmin) {
                        console.error('User is not an admin according to server');
                        logout(); // Force logout if server says user is not an admin
                    }
                })
                .catch(error => {
                    console.error('Error verifying admin status:', error);
                });
            
            return isAdmin;
        }
        
        // Logout function
        function logout() {
            // Clear localStorage
            localStorage.removeItem('userId');
            localStorage.removeItem('userEmail');
            localStorage.removeItem('userName');
            localStorage.removeItem('isAdmin');
            
            // Redirect to login page
            window.location.href = 'login.html';
        }
        
        // Load tab content
        function loadTab(tabName) {
            const contentArea = document.getElementById('adminContent');
            
            // Clear current content
            contentArea.innerHTML = '';
            
            // Load the appropriate content based on the tab
            switch(tabName) {
                case 'dashboard':
                    loadDashboardContent();
                    break;
                case 'donations':
                    loadDonationsContent();
                    break;
                case 'payments':
                    loadPaymentsContent();
                    break;
                case 'requirements':
                    loadRequirementsContent();
                    break;
                case 'feedback':
                    loadFeedbackContent();
                    break;
                case 'contacts':
                    loadContactContent();
                    break;
                case 'users':
                    loadUsersContent();
                    break;
                case 'settings':
                    loadSettingsContent();
                    break;
                default:
                    contentArea.innerHTML = '<h2>Page not found</h2>';
            }
        }
        
        // Dashboard content
        function loadDashboardContent() {
            const contentArea = document.getElementById('adminContent');
            
            // Create dashboard content
            const dashboardHTML = `
                <h2 class="page-title"><i class="fas fa-tachometer-alt"></i> Dashboard Overview</h2>
                
                <div class="stats-grid">
